<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.egoriku.activityresult">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ActivityResultAPI">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".image.ImagePickerActivity" />
        <activity android:name=".image.legacy.ImagePickerLegacy" />
        <activity android:name=".image.separate.ImagePickerActivitySplit" />
        <activity android:name=".permissions.PermissionsActivity" />
        <activity android:name=".customContract.MessageActivity" />
        <activity android:name=".customContract.CreateMessageActivity" />
    </application>

</manifest>